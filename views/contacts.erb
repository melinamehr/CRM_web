  <h2>
    There are <span class="count"><%= Contact.count%></span> contacts
  </h2>

    <% @contacts.each do |contact| %>
  <p>
    <a href="/contacts/<%=contact.id%>"><%= contact.full_name %></a>
    (<%= contact.email %>)
    <span class="edit">  <a href="/contacts/<%=contact.id%>/edit">Edit</a> | Delete</span>
  </p>
  <% end %>
